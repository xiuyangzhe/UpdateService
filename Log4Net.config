<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>

  <log4net>
    <logger name="logdebug">
      <level value="DEBUG"/>
      <appender-ref ref="DebugAppender"/>
    </logger>
    <logger name="loginfo">
      <level value="INFO"/>
      <appender-ref ref="InfoAppender"/>
    </logger>
    <logger name="logwarn">
      <level value="WARN"/>
      <appender-ref ref="WarnAppender"/>
    </logger>
    <logger name="logerror">
      <level value="ERROR"/>
      <appender-ref ref="ErrorAppender"/>
    </logger>
    <logger name="logfatal">
      <level value="FATAL"/>
      <appender-ref ref="FatalAppender"/>
    </logger>

    <logger name="WarningDatail_Oracle">
      <level value="ALL" />
      <appender-ref ref="ADONetAppender" />
    </logger>

    <appender name="DebugAppender" type="log4net.Appender.RollingFileAppender">
      
      <!--日志的路径-->
      <param name="File" value="Log\\Debug"/>
      <!--是否覆盖，默认是追加true-->
      <param name="AppendToFile" value="true"/>
      <!--设置无限备份=-1 ，最大备份数为1000-->
      <param name="MaxSizeRollBackups" value="10"/>
      <!--每个文件的最大20480KB-->
      <param name="MaximumFileSize" value="20480KB"/>
      <!--日志文件名是否为静态-->
      <param name="StaticLogFileName" value="false"/>
      <param name="DatePattern" value="'.'yyyy-MM-dd-HH'.log'"/>
      <param name="RollingStyle" value="Composite"/>
      <!--输出文件格式-->
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="[%date] [%level] [%thread] - %m%n"/>
      </layout>
      <!--輸出文件格式説明：  
         %d      输出当前语句运行的时刻  
         %t      当前语句所在的线程  
         %-数字：表示该项的最小长度，如果不够，则用空格填充  
         %p      日志的当前优先级别，即DEBUG、INFO<、WARN…等  
         %m      输出的日志消息，如ILog.Debug(…)>输出的一条消息  
         %n      换行  
         %r      運行的時間    
         %c      当前日志对象的名称  
      -->
      <!--Log4net 多线程写入-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="InfoAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Log\\Info"/>
      <param name="AppendToFile" value="true"/>
      <param name="MaximumFileSize" value="20480KB"/>
      <param name="MaxSizeRollBackups" value="10"/>
      <param name="StaticLogFileName" value="false"/>
      <param name="DatePattern" value="'.'yyyy-MM-dd-HH'.log'"/>
      <param name="RollingStyle" value="Composite"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="[%date] [%level] [%thread] - %m%n"/>
      </layout>
      <!--Log4net 多线程写入-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="WarnAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Log\\Warn"/>
      <param name="AppendToFile" value="true"/>
      <param name="MaxSizeRollBackups" value="10"/>
      <param name="MaximumFileSize" value="10240KB"/>
      <param name="StaticLogFileName" value="false"/>
      <param name="DatePattern" value="'.'yyyy-MM-dd-HH'.log'"/>
      <param name="RollingStyle" value="Composite"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="[%date] [%level] [%thread] - %m%n"/>
      </layout>
      <!--Log4net 多线程写入-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="ErrorAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Log\\Error"/>
      <param name="AppendToFile" value="true"/>
      <param name="MaxSizeRollBackups" value="10"/>
      <param name="MaximumFileSize" value="10240KB"/>
      <param name="StaticLogFileName" value="false"/>
      <param name="DatePattern" value="'.'yyyy-MM-dd-HH'.log'"/>
      <param name="RollingStyle" value="Composite"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="[%date] [%level] [%thread] - %m%n"/>
      </layout>
      <!--Log4net 多线程写入-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="FatalAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Log\\Fatal"/>
      <param name="AppendToFile" value="true"/>
      <param name="MaxSizeRollBackups" value="10"/>
      <param name="MaximumFileSize" value="10240KB"/>
      <param name="StaticLogFileName" value="false"/>
      <param name="DatePattern" value="'.'yyyy-MM-dd-HH'.log'"/>
      <param name="RollingStyle" value="Composite"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="[%date] [%level] [%thread] - %m%n"/>
      </layout>
      <!--Log4net 多线程写入-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>

    <!--作业ID-->
    <parameter>
      <parameterName value=":modulename" />
      <dbType value="String" />
      <size value="20" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%property{ModuleName}"/>
      </layout>
    </parameter>

  </log4net>
</configuration>